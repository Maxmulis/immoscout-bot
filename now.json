{
    "version": 2,
    "name": "apartment-notifier",
    "env": {
        "DB_NAME": "@db_name",
        "DB_USERNAME": "@db_username",
        "DB_PASSWORD": "@db_password",
        "COLLECTION_NAME": "@collection_name",
        "IMMO_SEARCH_URL": "@immo_search_url",
        "BOT_TOKEN": "@bot_token",
        "CHAT_ID": "@chat_id"
    },
    "builds": [{
        "src": "**/*.py",
        "use": "@now/python"
    }],
    "routes": [{
        "src": "(.*)",
        "dest": "index.py"
    }],
    "github": {
        "enabled": true
      }
}